.confirmation-dialog {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, 0.48);
    justify-content: center;
    align-items: center;
}

.confirmation-dialog__frame {
    background-color: rgba(255, 255, 255, 255);
    border-left-color: rgba(0, 0, 0, 255);
    border-right-color: rgba(0, 0, 0, 255);
    border-top-color: rgba(0, 0, 0, 255);
    border-bottom-color: rgba(0, 0, 0, 255);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-radius: 5px;
    padding: 10px;
}

.confirmation-dialog__header {
}

.confirmation-dialog__buttons {
    flex-direction: row;
    justify-content: space-between;
}

.confirmation-dialog__confirm-button {
}

