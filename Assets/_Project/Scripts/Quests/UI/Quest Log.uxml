                    <Bindings>
                        <ui:DataBinding property="itemsSource" data-source-path="VisibleQuests" binding-mode="ToTarget" />
                        <DualityGame.UI.SelectionToDataSourceBinding property="_ignored_" target-name="QuestDetailsPage" clear-path="true"/>
                    </Bindings>
                </ui:ListView>
            </ui:VisualElement>
            <ui:VisualElement name="QuestDetailsPage" picking-mode="Ignore" style="flex-grow: 0; flex-basis: 48%; flex-shrink: 0; display: flex;">
