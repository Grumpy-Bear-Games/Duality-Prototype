                    </DualityGame.UI.CollectionView>
                    <DualityGame.UI.CollectionView name="Checkpoints" item-template="project://database/Assets/_Project/Scripts/Quests/UI/Quest%20Log%20-%20Checkpoint.uxml?fileID=9197481963319205126&amp;guid=80b7994ad5ad1f746afa2cbf5a15e3c1&amp;type=3#Quest Log - Checkpoint">
