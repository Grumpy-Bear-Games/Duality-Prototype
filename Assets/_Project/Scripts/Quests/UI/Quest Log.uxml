                    </DualityGame.UI.AspectRatioElement>
                    <ui:Label text="Label" name="Title" picking-mode="Ignore">
                        <Bindings>
                            <ui:DataBinding property="text" data-source-path="Title" binding-mode="ToTarget" />
                        </Bindings>
                    </ui:Label>
                    <DualityGame.UI.AspectRatioElement name="Stamp" class="header-image">
                        <Bindings>
                            <ui:DataBinding property="style.backgroundImage" data-source-path="Stamp" binding-mode="ToTarget" source-to-ui-converters="Sprite" />
                        </Bindings>
                    </DualityGame.UI.AspectRatioElement>
                </ui:VisualElement>
                <ui:ScrollView name="ScrollView" vertical-scroller-visibility="AlwaysVisible" horizontal-scroller-visibility="Hidden" vertical-page-size="1" picking-mode="Ignore" horizontal-page-size="1">
